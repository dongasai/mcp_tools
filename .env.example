APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost:34004

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# MCP Server Configuration
MCP_SERVER_NAME="MCP Tools Server"
MCP_SERVER_VERSION="1.0.0"
MCP_PROTOCOL_VERSION="1.0"
MCP_TRANSPORT=sse

# SSE Configuration
SSE_ENDPOINT="/mcp/sse/connect"
MCP_SERVER_HOST=localhost
MCP_SERVER_PORT=34004
SSE_HEARTBEAT_INTERVAL=30
SSE_CONNECTION_TIMEOUT=300
MCP_MAX_CONNECTIONS=1000

# CORS Configuration
MCP_CORS_ENABLED=true
MCP_CORS_ORIGINS="*"
MCP_CORS_HEADERS="Authorization, Agent-ID, Content-Type"

# Agent Access Control
MCP_ENABLE_ACCESS_CONTROL=true
MCP_DEFAULT_PERMISSIONS=read
MCP_TOKEN_EXPIRY=86400
MCP_MAX_AGENTS_PER_USER=10
MCP_REQUIRE_AGENT_ID=true

# MCP Capabilities
MCP_ENABLE_RESOURCES=true
MCP_ENABLE_TOOLS=true
MCP_ENABLE_PROMPTS=true
MCP_ENABLE_NOTIFICATIONS=true

# Feature Toggles
MCP_ENABLE_PROJECTS=true
MCP_ENABLE_TASKS=true
MCP_ENABLE_GITHUB=true
MCP_ENABLE_AGENTS=true

# GitHub Integration
GITHUB_TOKEN=
GITHUB_WEBHOOK_SECRET=
GITHUB_API_URL="https://api.github.com"
GITHUB_RATE_LIMIT=5000

# Audit and Logging
ENABLE_PERMISSION_AUDIT=true
AUDIT_LOG_RETENTION_DAYS=90
MCP_AUDIT_LOG_CHANNEL=daily

# Cache Configuration
MCP_CACHE_ENABLED=true
MCP_CACHE_PREFIX="mcp_"
MCP_CACHE_TTL=3600
MCP_CACHE_PROJECTS_TTL=3600
MCP_CACHE_TASKS_TTL=300
MCP_CACHE_GITHUB_TTL=900
MCP_CACHE_AGENTS_TTL=60

# Performance Settings
MCP_ASYNC_PROCESSING=true
MCP_QUEUE_CONNECTION=database
MCP_MEMORY_LIMIT="256M"
MCP_GC_INTERVAL=300
