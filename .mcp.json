{
    "mcpServers": {
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest",
                "--cdp-endpoint",
                "http://127.0.0.1:9222"
            ]
        },
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp"
            ],
            "env": {
                "DEFAULT_MINIMUM_TOKENS": ""
            },
            "alwaysAllow": [
                "resolve-library-id",
                "get-library-docs"
            ]
        },
        "testme": {
            "type": "streamable-http",
            "url": "http://127.0.0.1:34004/mcp?token=123456&agent_id=test-agent-001",
            "disabled": false,
            "alwaysAllow": [
                "*"
            ]
        },
        "sequentialthinking": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ],
            "alwaysAllow": [
                "test_connection"
            ]
        }
    }
}